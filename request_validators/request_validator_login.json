{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "register Validation",
    "required": ["email_id","password"],
    "properties": {
      "email_id":{
        "type": "string",
        "title": "emailid schema",
        "pattern": "^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:.[a-zA-Z0-9-]+)*$"
      },
      "password":{
          "type": "string",
          "title": "password schema"
      }
    }
  }